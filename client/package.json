    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "react-select": "1.0.0-rc.10",
    "react-toggle": "4.0.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "request": "^2.88.0",
    "sass-loader": "^7.0.1",
    "skygear": "^1.6.4",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "terser-webpack-plugin": "^1.1.0",
    "tinymce": "^4.7.10",
    "tlds": "^1.203.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "5.3.0",
    "tslint": "5.11.0",
    "tslint-loader": "3.5.4",
    "tslint-react": "3.6.0",
    "typescript": "3.1.6",
    "url-loader": "^1.1.2",
    "uuid": "3.1.0",
    "webpack": "4.26.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "postinstall": "/bin/sh scripts/postinstall.sh",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.{ts,tsx}'",
    "format": "prettier --write --list-different 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@oursky/tslint-oursky": "0.11.0",
    "@types/classnames": "2.2.3",
    "@types/jest": "21.1.5",
    "@types/js-yaml": "3.9.1",
    "@types/mime-types": "^2.1.0",
    "@types/moment-timezone": "^0.5.4",
    "@types/node": "8.0.47",
    "@types/prop-types": "15.7.0",
    "@types/query-string": "5.0.1",
    "@types/react": "16.1.0",
    "@types/react-dom": "16.0.6",
    "@types/react-dropzone": "4.1.0",
    "@types/react-numeric-input": "2.2.0",
    "@types/react-router-dom": "4.2.0",
    "@types/react-router-redux": "5.0.10",
    "@types/react-select": "1.0.59",
    "@types/tlds": "^1.199.0",
    "@types/uuid": "3.4.3",
    "prettier": "1.15.3",
    "ts-skygear": "oursky/ts-skygear#912cd45c6b453df18999d6acde6a0b56453e92a3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0
      },
      "./src/cmsConfig/": {
        "statements": 80
      }
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.test.json"
      }
    }
  }
}
